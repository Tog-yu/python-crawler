#chrom
options=webdriver.ChromeOptions()
prefs={
     'profile.default_content_setting_values': {
        'images': 2,//限制图片加载
        'javascript':2 //限制js加载
       }
}
options.add_experimental_option('prefs',prefs)
 
browser = webdriver.Chrome(chrome_options=options)
browser.get('http://www.c's/')
